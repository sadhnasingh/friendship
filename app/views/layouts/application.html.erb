<!DOCTYPE html>
<html>
  <head>
    <title>Fm</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= form_tag(friendships_frnd_path, :method => "get", id: "search-form") do %>
      <%= text_field_tag :search, params[:search],placeholder: "Search friends" %>
      <%= submit_tag 'Search', :email => nil %>
    <% end %>
 <%= yield %>
  </body>
</html>
<center><div id="user_nav">
 <% if user_signed_in? %> Signed in as <%= current_user.email %>.Not you?
<%= link_to "Sign out", destroy_user_session_path, :method=> :delete %>
<% else %>
<%= link_to "Sign up", new_user_registration_path %> or <%= link_to "sign in", new_user_session_path %>
<% end %>
</div></center>


